#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--)
	{int arr[10],k,sum=0;
	 for(int i=0;i<10;i++)
	  {cin>>arr[i];}
	 cin>>k;
	 
	 for(int i=9;i>=0;i--)
       { sum+=arr[i];
         if(sum>k )
          { cout<<i+1<<endl;
            break;}
         
      }	 
      
	}
	return 0;
}
